[gui]
[user]
        name = __________
        email = __________
[gui]
[credential]
        helper = cache

[alias]
        br = branch -a
        ci = commit
        co = checkout
        df = diff
        dc = diff --cached
        lg = log -p
        lol = log --graph --decorate --pretty=oneline --abbrev-commit
        lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
        st = status
        up = rebase

[core]
        excludesfile = m:/.gitignore_global

[color]
        branch = auto
        diff = auto
        interactive = auto
        status = auto

[push]
        default = simple
