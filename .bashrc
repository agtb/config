alias ls='ls --color=auto'
alias la='ls -a'
alias ll='ls -al'
alias lll='ls -l | less'
alias less='less -M'

alias grep='grep'
alias h='history'
alias j='jobs'

# version control
alias ga='git add .'
alias gc='git commit -m'
alias gd='git diff'
alias gdc='git diff --cached'
alias gs='git status'
alias gl='git log --stat=120'
